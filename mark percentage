int main()
{
 char ch;
 printf("enter a character");
 scanf("%c",&ch);
 ch= tolower(ch);
 if("ch='a'||ch='e'||ch='i'||ch='o'||ch='n'")
 {
  printf("%c*is a vowel \n",ch);
 }
  else if("<ch>='a'&&ch<='c'>")
 {
  printf("%c is a constant\n",ch);
 }
 else
 {
  printf("invalid input:");
 }
 return 0;
}
